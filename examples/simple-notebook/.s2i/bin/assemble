#!/bin/bash

set -eo pipefail
set -x

echo " --> assembling the jupyter notebook"
ls -la /tmp/src
ls -la /tmp/scripts

cp /tmp/src/.jupyter/jupyter_notebook_config.py /opt/app-root/src/.jupyter/jupyter_notebook_config.py
ls -la /opt/app-root/src
ls -la /opt/app-root/src/.jupyter

echo " --> falling back to the default script"
exec /usr/libexec/s2i/assemble

ls -la /opt/app-root/src